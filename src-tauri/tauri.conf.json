{
  "productName": "modint-calculator",
  "version": "0.0.0",
  "identifier": "com.modint.calculator.dev",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "modint-calculator",
        "width": 800,
        "height": 600,
        "minHeight": 200,
        "minWidth": 200,
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/128x128@2x.png", "icons/icon.ico"]
  }
}
